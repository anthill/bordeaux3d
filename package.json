    "es6-promise": "^1.0.0",
    "es6-promisify": "^1.1.1",
    "es6-set": "~0.1.0",
    "event-emitter": "^0.3.1",
    "express": "^4.8.3",
    "jsdom": "^1.0.1",
    "rbush": "^1.3.2",
    "socket.io": "^1.0.6",
    "socket.io-client": "^1.0.6",
    "xmldom": "^0.1.19",
    "three": "^0.68.0",
    "csv-parse": "0.0.6",
    "city-core": "git://github.com/anthill/city-core.git#master",
    "city-blocks": "git://github.com/anthill/city-blocks.git#master"
