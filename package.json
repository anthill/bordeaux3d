    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "typescript": "^1.0.1",
    "unzip": "^0.1.9",
    "commander": "^2.3.0",
    "tmp": "^0.0.24",
    "q": "^1.0.1",
    "glob": "^4.0.5",
    "graceful-fs": "^3.0.2",
    "watchify": "^1.0.2"
