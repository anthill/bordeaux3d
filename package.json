{
  "name": "Bordeaux-3d",
  "version": "0.1.0",
  "description": "A simple implementation of the backend for the bordeaux3d project",
  "repository": "https://github.com/anthill/bordeaux3d",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js -o app.js -d",
    "watch": "watchify index.js -o app.js -d -v",
    "start": "node server.js dev",
    "start-dev": "node server.js dev",
    "start-prod": "node server.js prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-docker": "docker build -t ants/bordeaux3d:v1 .",
    "run-docker": "docker run -d -e VIRTUAL_HOST=bordeaux3d.ants.builders -e VIRTUAL_PORT=9100 -p 9100:9100 ants/bordeaux3d:v1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
